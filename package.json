{
  "name": "my-vite-vue3",
  "private": true,
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "start": "ts-node-dev --respawn ./server/app.ts",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "report": "set report=true&& vite build",
    "eslint": "eslint --fix --ext .js --ext .jsx --ext .ts --ext .tsx --ext .vue src/",
    "stylelint": "stylelint **/*.{html,vue,css,scss}",
    "lint": "npm run typecheck && npm run eslint && npm run stylelint",
    "lint-fix": "npm run eslint -- --fix && npm run stylelint -- --fix",
    "prepare": "husky install",
    "commit:comment": "引导设置规范化的提交信息",
    "commit": "git-cz"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@koa/cors": "^3.3.0",
    "@koa/router": "^11.0.1",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@vueuse/core": "^8.7.5",
    "axios": "^0.27.1",
    "babel-eslint": "^10.1.0",
    "element-plus": "^2.1.11",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-connect": "^2.1.0",
    "mockjs": "^1.1.0",
    "path": "^0.12.7",
    "pinia": "^2.0.14",
    "rollup-plugin-visualizer": "^5.6.0",
    "unplugin-icons": "^0.14.7",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^4.3.8",
    "less": "^4.1.2",
    "postcss": "^8.4.12",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "stylelint": "^14.8.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.7",
    "xlsx": "^0.17.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint-plugin-n",
        "rollup",
        "vue"
      ]
    }
  }
}
